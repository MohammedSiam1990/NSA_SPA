

<style>
    .k-upload-status {
        display: none;
    }
</style>
<form class=" form" id="CompanyForm">
    <input type="hidden" id="CompanyId" />
    <input type="hidden" id="ApprovedBy" />
    <input type="hidden" id="ApprovedDate" />
    <input type="hidden" id="StatusId" />
    <input type="hidden" id="CreationDate" />

    <div class="row">
        <div class="col-md-12 col-lg-6">
            <div class="row">
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-12 col-lg-6">
                            <div class=" text-left form-group">
                                <div class="  input-wrap is-required">
                                    <label class="label-input-pos required-label" string="NameEn"></label>
                                    <input type="text" name="fname" class="form-control input-sm" id="Name" />


                                </div>
                                <div class=" mt-1 is-helpful" data-helper="enter_your_Company_Name" data-error="please_enter_your_Company_Name"></div>
                            </div>
                        </div>

                        <div class="col-md-12 col-lg-6">
                            <div class=" text-left form-group">
                                <div class="  input-wrap is-required">
                                    <label class="label-input-pos required-label" for="" string="NameAr">

                                    </label>
                                    <input type="text" name="fname" class="form-control input-sm" id="NameAr" />
                                    <!--<span class="icon validation small success hide">
                                        <span class="fa fa-check"></span>
                                    </span>
                                    <span class="icon validation small error hide">
                                        <span class="fa fa-remove"></span>
                                    </span>-->

                                </div>
                                <div class=" mt-1 is-helpful" data-helper="enter_your_Company_Name_AR" data-error="please_enter_your_Company_Name_Ar"></div>
                            </div>
                        </div>

                        <div class="col-md-12 col-lg-6">
                            <div class=" text-left form-group">
                                <div class="  input-wrap is-required">
                                    <label class="label-input-pos  required-label" for="" string="Country">

                                    </label>
                                    <select id="Country" name="Country" class="w-100 input-sm p-0 ">
                                    </select>


                                </div>

                            </div>
                        </div>

                        <div class="col-md-12 col-lg-6">

                            <div class=" text-left form-group">
                                <div class="  input-wrap is-required">
                                    <label class="label-input-pos required-label" for="" string="Email">

                                    </label>
                                    <input type="email" name="email" class="form-control input-sm" id="Email" />


                                </div>
                                <div class=" mt-1 is-helpful" data-helper="Enter_email_format_right" data-error="Please_enter_your_email"></div>
                            </div>
                        </div>
                        <div class="col-md-12 col-lg-6">

                            <div class=" text-left form-group">
                                <div class="  input-wrap ">
                                    <label class="label-input-pos " for="" string="City">

                                    </label>
                                    <select id="City" name="Country" placeholder="PleaseSelect" class="w-100 input-sm">
                                    </select>


                                </div>

                            </div>
                        </div>

                        <div class="col-md-12 col-lg-6">
                            <div class="input-group mb-3">
                                <div class="w-100">
                                    <label class="label-input-pos required-label" for="" string="PhoneNumber">

                                    </label>
                                    <div class="input-group-prepend input-wrap is-required direction" style="margin-bottom:10px;">
                                        <span class="input-group-text" id="Code" style="min-width:50px"></span>
                                        <input type="tel" name="phone" class="form-control direction" id="Phone" oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');" />

                                    </div>
                                    <div class="text-left is-helpful" data-helper="Enter_Your_phone_number" data-error="please_Enter_Your_phone_number">
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="col-md-12 col-lg-6">
                            <div class=" text-left form-group">
                                <div class="  input-wrap">
                                    <label class="label-input-pos " for="" string="Address"></label>
                                    <input type="text" name="address" class="form-control input-sm" id="Address" />


                                </div>
                                <div class=" mt-1 is-helpful" data-helper="enter_your_Address" data-error="Please_enter_your_Address"></div>
                            </div>
                        </div>





                        <div class="col-md-12 col-lg-6">
                            <div class=" text-left form-group">
                                <div class="  input-wrap ">
                                    <label class="label-input-pos " for="" string="Currency">

                                    </label>
                                    <select id="Currency" name="Currency" class="w-100 input-sm p-0">
                                    </select>

                                </div>
                                <div class=" mt-1 is-helpful" data-helper="Enter_email_format_right" data-error="Please_enter_your_email"></div>
                            </div>
                        </div>



                    </div>


                </div>
            </div>
        </div>

        <div class="col-md-12 col-lg-6">


            <div class="row ">
                <div class=" col-sm-12 pl-4 pr-4">

                    <div>
                        <div class="col-md-12 col-lg-12">
                            <div class="card">
                                <div class="card-body" style="text-align: center; padding: 10px;">

                                    <div>
                                        <img src="../../assets/images/bg1.png" id="preview" class="img-thumbnail" onerror="if (this.src != '../../assets/images/bg1.png') { this.src = '../../assets/images/bg1.png';$('#ImageName').val(''); }">
                                        <button id="delete-image" data-toggle="tooltip" data-placement="bottom" title="Delete" type="button" class="delete-image" onclick="DeleteImg()"><i class="metismenu-icon pe-7s-close" aria-disabled="true"></i></button>
                                    </div>
                                    <div class="row imgbtn mt-2 container m-0">

                                        <input type="file" name="files" id="photos" class="  btn  btn-primary p-0" style="max-height:20px" />
                                        <input type="hidden" id="ImageName" />

                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>

            </div>
        </div>

        <div CLASS="col-md-12 col-lg-12  mt-3">
            <div class="  form-group bttuncompany m-0">
                <button id="UpdateCompany" type="button" class="browse btn btn-lg btn-primary mx-auto  " string="Save" onclick="UpdateCompany()" style="width:300px"></button>
            </div>
        </div>
    </div>


    <div class="clearfix">&nbsp;</div>

    <div class="row">
        <div CLASS="col-md-12 ">
            <div class="alert alert-dismissible fade  " role="alert" id="ResultMessage">
                <div id="ResultTxt">

                </div>
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        </div>
    </div>





</form>
<script src="../../Scripts/pages/Company.js"></script>
<script src="../../assets/js/Validations/Validation.js"></script>

