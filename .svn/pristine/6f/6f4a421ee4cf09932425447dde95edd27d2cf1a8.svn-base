        margin-right: 10px;
    }
label[for=MenuBarBranches]::after {
    content: ' ';
    position: absolute;
    top: 0px;
    width: 40px;
    height: 40px;
    display: block;
    background: #0a5c80 url('data:image/png;base64,') no-repeat center center;
    pointer-events: none;
}
label[for=MenuBarBranches]::after {
    right: 8px;
    border-bottom-right-radius: 10px;
    border-top-right-radius: 10px;
}
.hamburger span {
    display: block;
    position: absolute;
    top: 44px;
    left: 18px;
    right: 18px;
    height: 8px;
    background: white;
}

.hamburger span::before,
.hamburger span::after {
    position: absolute;
    display: block;
    left: 0;
    width: 100%;
    height: 8px;
    background-color: #fff;
    content: "";
}

.hamburger-x span {
    transition: background 0s 0.2s ease;
}

.hamburger-x span::before,
.hamburger-x span::after {
    transition-duration: 0.3s, 0.2s;
    transition-delay: 0.2s, 0s;
}

.hamburger-x span::before {
    transition-property: top, transform;
}

.hamburger-x span::after {
    transition-property: bottom, transform;
}

.hamburger-x.is-active {
    background-color: #E74C3C;
}

.hamburger-x.is-active span {
    background: none;
}

.hamburger-x.is-active span::before {
    top: 0;
    transform: rotate(45deg);
}

.hamburger-x.is-active span::after {
    top: 0;
    transform: rotate(-45deg);
}

.hamburger-x.is-active span::before,
.hamburger-x.is-active span::after {
    transition-delay: 0s, 0.2s;
}

